generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Raid {
  raidId           String   @id
  raidTitle        String
  difficulty       String
  startAt          DateTime
  notes            String
  channelId        String
  messageId        String?
  scheduledEventId String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
